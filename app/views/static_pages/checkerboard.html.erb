<% provide(:title, 'Checkerboard') %>

<body>
  <header>
    <div class="square1" id="square1">
	  <div class="row">
        <div class="span3"><p>"Hello"</p></div>
	  </div>
    </div>
      <div class="square2" id="square2">
	  <div class="row">
        <div class="span3"><p style="color:#FF00FF">"Hello"</p></div>
	  </div>
    </div>
  </header>
</body>

<table>
  <tbody>
    <% @squares.each do |row| %>
      <tr>
        <% row.each do |column| %>
          <td>
            <%= column %>
          </td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>